<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".LoginPage">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="418dp"
        android:layout_height="305dp"
        android:layout_marginTop="16dp"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/onlinedoctoramico"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-7dp" />

    <RelativeLayout
        android:layout_width="413dp"
        android:layout_below="@id/imageView5"
        android:layout_height="429dp"
        android:background="#504AFA"
        tools:ignore="MissingConstraints"
        tools:layout_editor_absoluteX="-2dp"
        tools:layout_editor_absoluteY="303dp">


        <ImageView
            android:id="@+id/email"
            android:layout_width="60dp"
            android:layout_height="10dp"
            android:layout_marginStart="15dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="30dp"
            android:src="@drawable/email" />

        <EditText
            android:id="@+id/edtEmailAddress"
            android:layout_width="300dp"
            android:layout_height="50dp"
            android:layout_below="@id/email"
            android:textColor="@color/white"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="30dp"
            android:ems="10"
            android:inputType="textEmailAddress" />

        <ImageView
            android:id="@+id/password"
            android:layout_width="65dp"
            android:layout_height="15dp"
            android:layout_below="@id/edtEmailAddress"
            android:layout_alignParentStart="true"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="30dp"
            android:src="@drawable/password" />

        <EditText
            android:id="@+id/edtPassword"
            android:layout_width="351dp"
            android:layout_height="50dp"
            android:textColor="@color/white"
            android:layout_below="@id/password"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="30dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="30dp"
            android:ems="10"
            android:inputType="textPassword" />

        <ImageView
            android:id="@+id/txtforgotpassword"
            android:layout_width="154dp"
            android:layout_height="22dp"
            android:layout_below="@+id/edtPassword"

            android:layout_alignParentEnd="true"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:src="@drawable/forgeryourpassword" />

        <ImageButton
            android:id="@+id/btnSignUp"
            android:layout_width="385dp"
            android:layout_height="wrap_content"
            android:layout_below="@+id/txtforgotpassword"
            android:layout_marginStart="21dp"
            android:layout_marginEnd="5dp"
            android:layout_marginTop="10dp"
            android:background="@color/transparent"
            android:src="@drawable/signin" />

        <ImageView
            android:id="@+id/txtDont"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/btnSignUp"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="97dp"
            android:layout_marginTop="15dp"
            android:layout_marginEnd="182dp"
            android:src="@drawable/donthaveaccount" />


        <ImageButton
            android:id="@+id/btnLogin"
            android:layout_width="80dp"
            android:layout_height="20dp"
            android:layout_below="@+id/btnSignUp"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="13dp"
            android:layout_marginEnd="120dp"
            android:background="@color/transparent"
            android:src="@drawable/login" />

        <RelativeLayout
            android:id="@+id/orsignupwith"
            android:layout_width="200dp"
            android:layout_height="30dp"
            android:layout_below="@id/btnLogin"
            android:layout_marginStart="100dp"
            android:layout_marginTop="5dp"
            android:layout_marginEnd="100dp">

            <ImageView
                android:layout_width="40dp"
                android:layout_height="15dp"

                android:layout_marginStart="5dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="115dp"
                android:src="@drawable/linefive">


            </ImageView>

            <ImageView
                android:layout_width="100dp"
                android:layout_height="20dp"
                android:layout_marginStart="50dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="50dp"
                android:src="@drawable/orsignupwith">


            </ImageView>

            <ImageView
                android:layout_width="40dp"
                android:layout_height="15dp"
                android:layout_alignParentEnd="true"
                android:layout_marginStart="115dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="5dp"
                android:src="@drawable/linesix">


            </ImageView>
        </RelativeLayout>

        <ImageButton
            android:id="@+id/btnGoogle"
            android:layout_width="wrap_content"
            android:layout_height="42dp"
            android:layout_below="@+id/orsignupwith"
            android:layout_alignParentStart="true"
            android:layout_alignParentEnd="true"
            android:layout_alignParentBottom="true"
            android:layout_marginStart="174dp"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="179dp"
            android:layout_marginBottom="20dp"
            android:background="@color/transparent"
            android:src="@drawable/googleimage"></ImageButton>


    </RelativeLayout>

</RelativeLayout>